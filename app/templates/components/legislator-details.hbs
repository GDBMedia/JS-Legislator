<div class="row">
  <div class="col s6">
    <button class="btn btn-large cyan" {{action 'toggleAction'}}>Committees</button>
  </div>
  <div class="col s6">
    <button class="btn btn-large cyan" {{action 'toggleAction'}}>Bills</button>
  </div>
</div>
{{#if toggle}}
{{log id}}
{{log key}}
{{log url1}}
{{log bills}}
{{#each model.committees as |committee|}}
<div class="card hoverable">
  <div class="card-content">
    <span class="card-title activator grey-text text-darken-4">{{committee.name}}<i class="material-icons right">more_vert</i></span>
    <p>Chamber: {{committee.chamber}}</p>
    <br>
    {{#if committee.subcommittee}}
      <p>Subcommittee</p>
    {{else}}
      <p>Committee</p>
    {{/if}}
  </div>
  <div class="card-action">
    {{#link-to 'list-bills' committee.committee_id}}<a>List of Bills</a>{{/link-to}}
  </div>
</div>
{{/each}}
{{else}}
  {{log "ho"}}
  {{#each model.bills as |bill|}}
    {{bill-list bill=bill}}
  {{/each}}
{{/if}}
